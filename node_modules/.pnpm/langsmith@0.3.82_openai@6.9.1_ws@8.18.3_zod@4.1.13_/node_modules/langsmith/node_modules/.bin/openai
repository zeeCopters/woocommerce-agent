#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zeeshan/Desktop/Node/woocommerce-agent/node_modules/.pnpm/openai@6.9.1_ws@8.18.3_zod@4.1.13/node_modules/openai/bin/node_modules:/Users/zeeshan/Desktop/Node/woocommerce-agent/node_modules/.pnpm/openai@6.9.1_ws@8.18.3_zod@4.1.13/node_modules/openai/node_modules:/Users/zeeshan/Desktop/Node/woocommerce-agent/node_modules/.pnpm/openai@6.9.1_ws@8.18.3_zod@4.1.13/node_modules:/Users/zeeshan/Desktop/Node/woocommerce-agent/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zeeshan/Desktop/Node/woocommerce-agent/node_modules/.pnpm/openai@6.9.1_ws@8.18.3_zod@4.1.13/node_modules/openai/bin/node_modules:/Users/zeeshan/Desktop/Node/woocommerce-agent/node_modules/.pnpm/openai@6.9.1_ws@8.18.3_zod@4.1.13/node_modules/openai/node_modules:/Users/zeeshan/Desktop/Node/woocommerce-agent/node_modules/.pnpm/openai@6.9.1_ws@8.18.3_zod@4.1.13/node_modules:/Users/zeeshan/Desktop/Node/woocommerce-agent/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../openai@6.9.1_ws@8.18.3_zod@4.1.13/node_modules/openai/bin/cli" "$@"
else
  exec node  "$basedir/../../../../../openai@6.9.1_ws@8.18.3_zod@4.1.13/node_modules/openai/bin/cli" "$@"
fi
